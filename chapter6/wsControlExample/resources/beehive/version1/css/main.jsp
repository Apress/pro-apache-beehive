body { background: #EFEEE2 url("/mySpt/framework/skins/myspt/images/bg_main.gif");
background-repeat: repeat-x; font-size:11px; line-height:12px; margin: 0 }
a:link		{ color:#369; text-decoration:none }
a:visited	{ color:#036; text-decoration:none }
a:active	{ color:#69c; text-decoration:underline }
a:hover		{ color:#69c; text-decoration:underline }
.promo, .promotitle, .text, .timestamp, .subhead, .tab, .subnav, .subnavon,
.note, .form, .gain, .loss, .ticka, .ondark, .error,
.breadcrumb, .sort, .readitonly, .readitonly2 { font-family:'tahoma','verdana', sans-serif }
.promo, .promotitle, .text, .headline, .breadcrumb, .form, .subnavon { color:#4c4c4c }
.note 		{ color:#888 }
.gain		{ color:#393 }
.loss, .error	{ color:#933 }
.gray		{ color:#888 }
.sort		{ font-weight:bold }
.sort a:link, .sort a:visited { color:#369 }
.sort a:active, .sort a:hover { color:#69c }
.ondark a:link, .ondark a:visited { color:#9cf }
.ondark a:active, .ondark a:hover { color:#69c }
.breadcrumb	{ color:#fff }
.breadcrumb a:link, .breadcrumb a:visited { color:#fff; text-decoration:underline }
.breadcrumb a:active, .breadcrumb a:hover { color:#E0EEF7 }
.error a:link, .error a:visited { color:#933 }
.error a:active, .error a:hover { color:#C66 }
table.portlet { border: solid 1px #bababa; border-right:0; border-bottom:0 }
table.subportlet { border: solid 1px #D6D4C8 }
td.corphead	{ background-color:#7790A4; font-family:tahoma; font-size:10px; font-weight:bold; color:#fff; text-transform:uppercase; }
td.corpsubhead	{ background: #D6D4C8 }
td.mphead	{ background: #34544D url("/mySpt/framework/skins/myspt/images/ph_mp.gif"); background-repeat:repeat-x; font-family:tahoma; font-size:10px; font-weight:bold; color:#fff; text-transform:uppercase; }
td.tvhead	{ background: #63513D url("/mySpt/framework/skins/myspt/images/ph_tv.gif"); background-repeat:repeat-x; font-family:tahoma; font-size:10px; font-weight:bold; color:#fff; text-transform:uppercase; }
td.homeenthead	{ background: #58435B url("/mySpt/framework/skins/myspt/images/ph_homeent.gif"); background-repeat:repeat-x; font-family:tahoma; font-size:10px; font-weight:bold; color:#fff; text-transform:uppercase; }
td.prodhead	{ background: #445769 url("/mySpt/framework/skins/myspt/images/ph_prod.gif"); background-repeat:repeat-x; font-family:tahoma; font-size:10px; font-weight:bold; color:#fff; text-transform:uppercase; }
tr.sort		{ background-color:#D6D4C8 }
tr.odd		{ background-color:#fff }
tr.even		{ background-color:#F7F6EA }
td.today	{ background-color:#C5C3B8 }
td.weekend	{ background-color:#E3E2D7 }
td.meeting	{ background-color:#E3E2D7 }
td.tabfront	{ background-color:#EFEEE2 }
.tabfront a:link, .tabfront a:visited { color:#4c4c4c; text-decoration:none }
.tabfront a:active, .tabfront a:hover { color:#369; text-decoration:underline }
td.tabback	{ background-color:#369 }
.tabback a:link, .tabback a:visited { color:#fff; text-decoration:none }
.tabback a:active, .tabback a:hover { color:#9CF; text-decoration:underline }
table.ttab td	{ font-size:10px; font-weight:bold; text-transform:uppercase; border-collapse:collapse }
.ttab a:link, .ttab a:visited { color:#fff; text-decoration:none }
.ttab a:active, .ttab a:hover { color:#9CF; text-decoration:underline }
td.ttabfront	{ background-color:#fff; border: solid 1px #999; border-bottom: solid 1px #fff }
td.ttabback	{ background-color:#369; border: solid 1px #999 }
table.prefbox { background-color:#fff; border-left: solid 1px #999 }
.tab		{ font-weight:bold; text-transform:uppercase }
.tab a:link, .tab a:visited { color:#fff; text-decoration:none }
.tab a:active, .tab a:hover { color:#9CF; text-decoration:underline }
td.tabfront	{ background-color:#EFEEE2 }
td.tabback	{ background-color:#369 }
.subnav		{ font-weight:bold; text-transform:uppercase }
.subnav a:link, .subnav a:visited	{ color:#fff; text-decoration:none }
.subnav a:active, .subnav a:hover	{ color:#9cf; text-decoration:underline }
.subnavon	{ font-weight:bold; text-transform:uppercase }
td.subnav	{ background-color:#369 }
td.subnavon { background-color:#D6D4C8 }
.thirdnav	{ font-weight:normal; text-transform:none }
.thirdnav a:link, .thirdnav a:visited	{ color:#369; text-decoration:none }
.thirdnav a:active, .thirdnav a:hover	{ color:#69c; text-decoration:underline }
.nav		{ font-family:'tahoma','arial',sans-serif; font-size:10px; color:#ababab; font-weight:bold; text-transform:uppercase }
.nav a:link, .nav a:visited { color:#ffc; text-decoration:none; text-transform:uppercase }
.nav a:active, .nav a:hover { color:#cc9; text-decoration:none; text-transform:uppercase }

/*business objects workarounds*/
span.report table td { padding:3px }
span.report table { width:100% }
span.reportfull table td { padding:6px }
span.reportfull table { width:100% }

/*legacy SPT styles*/
.ticka		{ font-size:11px; line-height:13px; color:#bababa }
.ticka a:link, .ticka a:visited { color:#9cf }
.ticka a:active, .ticka a:hover { color:#69c }
td.promohead	{ background-color:#7790A4 }

/* BEA stuff */

.bea-portal-ie-table-buffer-div { width: 100%; }
form { margin: 0px; padding: 0px; }
input, select, textarea { margin: 2px; font-size: small; }

/*the following changes on NS4 for PC*/

.note, .form, .sort, .breadcrumb	{ font-size:9px; line-height:11px }
.promo, .error, .tab, .subnav, .subnavon 	{ font-size:10px; line-height:12px }
.promotitle	{ font-size:12px; line-height:14px; font-weight:bold }
.text	 	{ font-size:11px; line-height:14px }
.subhead	{ font-size:12px; line-height:14px; font-weight:bold }
.headline 	{ font-family:'tahoma','arial', sans-serif; font-size:18px; line-height:20px; font-weight:bold }
.header 	{ font-family:'tradegothic lh extended','arial', sans-serif; font-size:20px; line-height:24px; font-weight:bold; text-transform:uppercase; color:#CFDCE5 }
.timestamp 	{ font-size:9px; line-height:11px; color:#888 }
.breaker	{ font-size:3px; line-height:3px }
.ondark		{ font-size:10px; line-height:12px; color:#bababa }
input.readitonly { color:#4c4c4c; border-width:0; background-color:#fff; font-size:11px; line-height:13px }
input.readitonly2 { color:#4c4c4c; border-width:0; background-color:#F7F6EA; font-size:11px; line-height:13px }

table { border-collapse:collapse }

table.prefbox { background-color:#fff; border-left: solid 1px #999 }


.tab { font-weight:bold; text-transform:uppercase }

table.ttab td	{ font-size:10px; font-weight:bold; text-transform:uppercase; border-collapse:collapse }

td.ttabfront	{ background-color:#fff; border: solid 1px #999; border-bottom: solid 1px #fff }
.ttabfront a:link, .ttabfront a:visited { color:#000; text-decoration:none }

td.ttabback	{ background-color:#369; border: solid 1px #999 }
.ttabback a:link, .ttabback a:visited { color:#fff; text-decoration:none }
.ttabback a:active, .ttabback a:hover { color:#9CF; text-decoration:underline }
